<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterAsPerformer">

    <TextView
        android:id="@+id/LastName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="56dp"
        android:text="Фамилия"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edit_LastName"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        app:layout_constraintStart_toEndOf="@id/LastName"
        app:layout_constraintTop_toTopOf="@id/LastName" />

    <TextView
        android:id="@+id/Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="Имя"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/LastName"
        app:layout_constraintTop_toBottomOf="@id/LastName"/>

    <EditText
        android:id="@+id/edit_Name"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="122dp"
        app:layout_constraintStart_toEndOf="@id/Name"
        app:layout_constraintTop_toTopOf="@id/Name" />

    <TextView
        android:id="@+id/MiddleName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Отчество"
        android:textSize="25sp"
        android:layout_marginTop="35dp"
        app:layout_constraintStart_toStartOf="@+id/Name"
        app:layout_constraintTop_toBottomOf="@id/Name"/>

    <EditText
        android:id="@+id/edit_MiddleName"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        app:layout_constraintStart_toEndOf="@id/MiddleName"
        app:layout_constraintTop_toTopOf="@id/MiddleName" />

    <TextView
        android:id="@+id/Age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="Возраст"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/MiddleName"
        app:layout_constraintTop_toBottomOf="@id/MiddleName"/>

    <EditText
        android:id="@+id/edit_Age"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        app:layout_constraintStart_toEndOf="@id/Age"
        app:layout_constraintTop_toTopOf="@id/Age" />

    <TextView
        android:id="@+id/University"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="ВУЗ"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/Age"
        app:layout_constraintTop_toBottomOf="@id/Age" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        app:layout_constraintStart_toEndOf="@id/University"
        app:layout_constraintTop_toTopOf="@id/University"/>

    <TextView
        android:id="@+id/YearStudy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="Год обучения"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/University"
        app:layout_constraintTop_toBottomOf="@id/University"/>

    <EditText
        android:id="@+id/edit_YearStudy"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        app:layout_constraintStart_toEndOf="@id/YearStudy"
        app:layout_constraintTop_toTopOf="@id/YearStudy" />

    <TextView
        android:id="@+id/Login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="Логин"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/YearStudy"
        app:layout_constraintTop_toBottomOf="@id/YearStudy"/>

    <EditText
        android:id="@+id/edit_Login"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        app:layout_constraintStart_toEndOf="@id/Login"
        app:layout_constraintTop_toTopOf="@id/Login" />

    <TextView
        android:id="@+id/Password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="Пароль"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/Login"
        app:layout_constraintTop_toBottomOf="@id/Login"/>

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        app:layout_constraintStart_toEndOf="@id/Password"
        app:layout_constraintTop_toTopOf="@id/Password" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="зарегистрироваться"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Password"
        android:textSize="18sp"/>

</androidx.constraintlayout.widget.ConstraintLayout>